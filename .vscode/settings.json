{

    "python.pythonPath": "/usr/bin/python3",
    "python.autoComplete.extraPaths": [
        "src/classifier/src/cpp_wrappers/cpp_neighbors",
        "src/classifier/src/cpp_wrappers/cpp_subsampling",
        "src/classifier/src/cpp_wrappers",
        "src/classifier",
        "/opt/ros/noetic/lib",
        "/opt/ros/noetic/lib/python3/dist-packages",
        "/opt/ros/noetic/lib/python3/dist-packages/rospy",
        "/opt/ros/noetic/lib/**",
        "/home/hth/catkin_ws/devel/lib/python3/dist-packages"
    ],

    "python.formatting.provider": "autopep8",
    "python.formatting.autopep8Args": [
        "--ignore=E302,E265,E306",
        "--max-line-length=200"
    ],

    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        ".catkin_tools/**": true,
        "build/**": true,
        "imgs/**": true,
        "scripts/**": true,
        "logs/**": true
    
    },

    "python.linting.enabled": true,
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,

}